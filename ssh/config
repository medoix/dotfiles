Host gitlab.com
    HostName gitlab.com
    UpdateHostKeys no
    IdentityFile ~/.ssh/gitlab
    IdentitiesOnly yes

Host github.com
    HostName github.com
    UpdateHostKeys no
    IdentityFile ~/.ssh/github
    IdentitiesOnly yes

Host github.com-work
    HostName github.com
    UpdateHostKeys no
    IdentityFile ~/.ssh/github-work
    IdentitiesOnly yes

